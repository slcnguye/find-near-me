{
  "manifest_version": 2,

  "name": "find near me",
  "description": "Search for anything and we will map it for you.",
  "version": "1.3.2",
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "icons": { 
    "16": "resources/Pin_16X16.png",
    "128": "resources/Pin_128X128.png" 
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ]
}